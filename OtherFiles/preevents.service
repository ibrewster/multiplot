[Unit]
Description = PREEVENTS multidisciplinary graphing tool
After = network.target

[Service]
ExecStart = /apps/prevents/env/bin/gunicorn -c /apps/prevents/OtherFiles/gunicorn.conf.py
WorkingDirectory=/apps/prevents
User=nginx
Group=nginx
RuntimeDirectory = preevents
RuntimeDirectoryMode = 777
Restart=on-failure
RestartSec=15s

[Install]
WantedBy = multi-user.target
